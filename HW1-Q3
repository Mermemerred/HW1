#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main( void )
{
   int popularity ;
   int size ;
   int HomeValue;     

   printf(" Enter popularity\n" );
   scanf( "%d", &popularity );       

   printf( "Enter size\n" ); 
   scanf( "%d", &size );          
   
   HomeValue = (pow(popularity,3)+ pow(size,2))*1000; /* calculate the price*/
   
   printf( "Your value is: %d\n", HomeValue ); 
   system("PAUSE");
   return 0;

} 
